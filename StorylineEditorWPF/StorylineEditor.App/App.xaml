<Application x:Class="StorylineEditor.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/Icons.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/Tags.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/Tooltips.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/Colors.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/Dimensions.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/Fonts.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/Base.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/Props.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Style/Other.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>